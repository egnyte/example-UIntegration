{
    "appID": "exampleuint",
    "name": "Example UI Integration",
    "author": "Your mum",
    "support": {
        "web": "http://xkcd.com"
    },
    "type": "both",
    "appLogo": "http://goo.gl/F15gjA",
    "description": "Example UI Integration for testing purposes. Contains both kinds of invocation pattern",
    "screenshots": {
        "images": ["http://imgs.xkcd.com/comics/antique_factory.png", "http://imgs.xkcd.com/comics/the_bdlpswdks_effect.png", "http://imgs.xkcd.com/comics/placebo_blocker.png"]
    },
    "userSettings": "http://xkcd.com",
    "accessLevel": "fileSystem",
    "integration": [{
        "allowedPaths": "/Shared/*",
        "entryPoint": "context_menu",
        "types": ["file"],
        "accessLevel": "selection",
        "serviceURL ": "https: //integrations-staging.qa-egnyte.com/exampleuint/service",
        "extensions": ["png", "jpg"],
        "permissionLevels": ["Owner", "full"],
        "selectionQuantity": "single",
        "invoke": "remote",
        "item": {
            "text": "Remote service call"
        }
    }, {
        "entryPoint": "context_menu",
        "hostingURL": "https://integrations-staging.qa-egnyte.com/exampleuint/staticapp",
        "types": ["file"],
        "extensions": ["png"],
        "selectionQuantity": [2, 5],
        "invoke": "hosted",
        "item": {
            "text": "Hosted app",
            "tooltip": "hello"
        }
    }]

}